      <script>
        function updateQuantityOnServer(inputElement) {
          var newQuantity = inputElement.value; // Assuming this is triggered by an event where `inputElement` is the quantity input
          var itemId = inputElement.getAttribute('data-item-id');
          fetch('/update-quantity-in-productPage', {
            method: 'POST',
            headers: {
              'Content-Type': 'application/json',
            },
            body: JSON.stringify({ itemId: itemId, quantity: newQuantity }),
          })
            .then(response => response.json())
            .then(data => {
              if (data.success) {
                // Update the DOM elements with new values
                document.getElementById('totalPriceElement').innerText = data.totalPrice.toFixed(2);
                document.getElementById('discountPriceElement').innerText = data.discountPrice.toFixed(2);
                document.getElementById('discountFinalElement').innerText = data.totalPayable.toFixed(2);
                document.getElementById('errorMessage').innerText = data.message = '';

                document.getElementById('proceedToPaymentBtn').disabled = false;

              } else {
                // Handle error, show message to user
                document.getElementById('errorMessage').style.display = 'block';
                document.getElementById('errorMessage').innerText = data.message;
                document.getElementById('proceedToPaymentBtn').disabled = true;

              }
            })
            .catch(error => {
              console.error('Error updating quantity:', error);
              // Optionally handle/display this error on the UI
            });



          function checkItemsStockAndAdjustPaymentButton() {
            var allQuantitiesValid = true;
            var quantityInputs = document.querySelectorAll('.quantityInput');
            quantityInputs.forEach(function (input) {
              var quantity = parseInt(input.value);
              var maxStock = parseInt(input.getAttribute('data-max-stock')); // Assuming you have data-max-stock attribute
              if (quantity > maxStock) {
                allQuantitiesValid = false;
              }
            });

            // Enable or disable the Proceed to Payment button based on stock availability
            var proceedToPaymentBtn = document.getElementById('proceedToPaymentBtn');
            if (allQuantitiesValid) {
              proceedToPaymentBtn.disabled = false;
            } else {
              proceedToPaymentBtn.disabled = true;
            }
          }
        }
      </script>


<script>
  document.getElementById('reviewForm').addEventListener('submit', function(event) {

  // Clear previous errors
  document.querySelectorAll('.error').forEach(function(el) {
    el.classList.remove('error');
  });
  document.querySelectorAll('.invalid-feedback').forEach(function(el) {
    el.style.display = 'none';
  });

  let isValid = true;

  // Rating validation
  const rating = document.querySelector('input[name="rate"]:checked');
  if (!rating) {
    document.querySelector('#rateFeedback').textContent = 'Please select a rating.';
    document.querySelector('#rateFeedback').style.display = 'block';
    isValid = false;
  }

  // Message validation
  const message = document.getElementById('message');
  if (!message.value.trim()) {
    document.getElementById('messageFeedback').textContent = 'Message cannot be empty.';
    document.getElementById('messageFeedback').style.display = 'block';
    message.classList.add('error');
    isValid = false;
  }

  // Name validation
  const name = document.getElementById('name');
  if (!name.value.trim()) {
    document.getElementById('nameFeedback').textContent = 'Name is required.';
    document.getElementById('nameFeedback').style.display = 'block';
    name.classList.add('error');
    isValid = false;
  }

  // Email validation
  const email = document.getElementById('email');
  const emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
  if (!email.value.trim() || !emailRegex.test(email.value)) {
    document.getElementById('emailFeedback').textContent = 'Invalid email address.';
    document.getElementById('emailFeedback').style.display = 'block';
    email.classList.add('error');
    isValid = false;
  }

  // If all validations pass, submit the form or proceed with further actions
  if (isValid) {
    // document.getElementById('reviewForm').submit();
    console.log('Form is valid. Submitting...');
    
  }
});

</script>


