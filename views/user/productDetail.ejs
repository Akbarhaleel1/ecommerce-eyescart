    document.addEventListener('DOMContentLoaded', function() {
      const reviews = document.querySelectorAll('.user-review.hidden');
      let currentIndex = 0;
    
      document.getElementById('loadMore').addEventListener('click', function() {
        const maxIndex = currentIndex + 3;
        for(; currentIndex < maxIndex && currentIndex < reviews.length; currentIndex++) {
          reviews[currentIndex].classList.remove('hidden');
        }
        // Hide the button if there are no more reviews to show
        if (currentIndex >= reviews.length) {
          this.style.display = 'none';
        }
      });
    });
    </script>
    


