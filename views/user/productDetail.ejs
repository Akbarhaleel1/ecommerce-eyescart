      <script>
        document.addEventListener('DOMContentLoaded', function () {
          var plusButtons = document.querySelectorAll('.plusBtn');
          var minusButtons = document.querySelectorAll('.minusBtn');

          plusButtons.forEach(function (btn) {
            btn.addEventListener('click', function () {
              var input = this.parentNode.querySelector('.quantityInput');
              var newQuantity = parseInt(input.value) + 1;
              input.value = newQuantity;
              updateQuantityOnServer(input, newQuantity);
            });
          });

          minusButtons.forEach(function (btn) {
            btn.addEventListener('click', function () {
              var input = this.parentNode.querySelector('.quantityInput');
              if (input.value > 1) {
                var newQuantity = parseInt(input.value) - 1;
                input.value = newQuantity;
                updateQuantityOnServer(input, newQuantity);
              }
            });
          });


        });
      </script>


